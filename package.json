{
  "name": "magnit-agent-front",
  "version": "1.0.0",
  "main": "./index.tsx",
  "author": "KTS Studio",
  "license": "MIT",
  "scripts": {
    "rm-source-maps": "find ./public -name *.js.map -type f -delete",
    "dev": "NODE_ENV=development API_URL=/api/ webpack serve --mode development",
    "win-dev": "SET NODE_ENV=development& SET API_URL=/api/& webpack serve --mode development",
    "build": "yarn rm-public & NODE_ENV=production webpack --mode production && yarn run rm-source-maps",
    "husky:prepare": "npx husky-init && npx husky add .husky/pre-commit \"yarn lint-staged\" && chmod ug+x .husky/* && chmod ug+x .git/hooks/*",
    "tsc:check": "tsc --noEmit",
    "codestyle:fix": "prettier --write **/*.{ts,tsx,json,css,scss}",
    "lint": "eslint src --ext .js,.jsx,ts,.tsx",
    "lint:fix": "eslint --fix src --ext .js,.jsx,ts,.tsx",
    "stylelint": "stylelint **/*.{css,scss}",
    "stylelint:fix": "stylelint --fix **/*.{css,scss}",
    "precommit": "yarn lint-staged",
    "format": "yarn lint:fix && yarn stylelint:fix && yarn codestyle:fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js}?(x)": "yarn lint",
    "*.?(s)css": "yarn stylelint"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.7",
    "@dr.pogodin/babel-plugin-react-css-modules": "^6.8.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@sentry/webpack-plugin": "^1.18.3",
    "@svgr/webpack": "^6.2.0",
    "@types/react": "^18.0.12",
    "@types/react-css-modules": "^4.6.4",
    "@types/react-dom": "^17.0.11",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^12.1.7",
    "mini-css-extract-plugin": "^2.4.6",
    "postcss": "^8.3.9",
    "postcss-loader": "^6.2.1",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.4.3",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "react-is": "^17.0.2",
    "react-refresh": "^0.11.0",
